				<key>applescript</key>
				<string>on alfred_script(q)
  set recipientName to "iDoneThis Wistia"
  set recipientAddress to "wistia@team.idonethis.com"
  set theBody to q
  set theSubject to "New iDoneThis item"
  set theSender to "Robby Grossman &lt;robby@wistia.com&gt;"

  tell application "Mail"
    set theMessage to make new outgoing message with properties {subject: theSubject, content: theBody, visible: true}
    tell theMessage
      set sender to theSender
      make new to recipient with properties {name: recipientName, address: recipientAddress}
      send
    end tell
  end tell
end alfred_script
</string>
